{
    "name": "enniel/ami-log",
    "type": "library",
    "description": "Logger for ami",
    "keywords": [
        "Enniel",
        "Ami",
        "Asterisk"
    ],
    "homepage": "https://github.com/enniel/ami-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Enniel"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/events": "~5.1",
        "illuminate/database": "~5.1",
        "enniel/ami": "~2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.0",
        "react/promise": "~2.4.1",
        "orchestra/testbench": "~3.1.8|~3.2.6|~3.3.2",
        "orchestra/database": "~3.1",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Enniel\\AmiLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enniel\\AmiLog\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "php vendor/bin/phpunit",
        "phpcs": "php vendor/bin/php-cs-fixer --diff --dry-run -v fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
